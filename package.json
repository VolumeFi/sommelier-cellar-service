{
  "name": "sommelier-cellar-service",
  "version": "1.0.0",
  "main": "server",
  "private": true,
  "scripts": {
    "start": "node .",
    "test": "jest",
    "build": "babel src -s -D -d dist --presets es2015,stage-1",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "lint": "eslint server",
    "docs": "apidoc -i server -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "opn docs/index.html"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "devDependencies": {
    "ansi-regex": "^6.0.1",
    "apidoc": "^0.29.0",
    "apidoc-markdown": "^6.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "nock": "^13.1.3",
    "nodemon": "^2.0.12"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "bodymen": "^1.0.3",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-force-ssl": "^0.3.2",
    "mixpanel": "^0.13.0",
    "mongoose": "^6.4.6",
    "morgan": "^1.10.0",
    "querymen": "^2.1.3",
    "web-push": "^3.4.5"
  },
  "resolutions": {
    "ansi-regex": "^6.0.1"
  }
}
